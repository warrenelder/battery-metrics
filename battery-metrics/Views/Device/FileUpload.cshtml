@{
    ViewData["Title"] = "Device Data Upload";
}
<div class="row">
    <div class="col-md-4">
        <h3>@ViewData["Title"]</h3>
    </div>
</div>

<div class="row">
    <div class="col-md-4 col-lg-12">
        @if(@ViewData["Message"] != null){
            <p>@ViewData["Message"]</p>
        }
    </div>
</div>

<div class="row">
    <div class="col-md-4 col-lg-12">
        <form asp-controller="Device" asp-action="FileUpload" method="post" enctype="multipart/form-data">
            <div class="form-group inline">
                <label class="control-label">File</label>
                <input name="file" type="file" class="form-control" style="height:auto" />
            </div>
            <a class="btn btn-primary inline" asp-controller="Device" asp-action="Index">Back</a>
            <input type="submit" value="Upload" class="btn btn-default inline" />
        </form>
    </div>
</div>